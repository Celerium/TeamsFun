    <command:examples>
      <command:example>
        <maml:title>-------------------------- EXAMPLE 1 --------------------------</maml:title>
        <dev:code>.\Send-TeamsPurr.ps1 -TeamsURI 'https://outlook.office.com/webhook/123/123/123/.....'</dev:code>
        <dev:remarks>
